{
  "name": "notepad",
  "version": "1.0.0",
  "description": "Collabrative Notepad that use js-crdt as a base.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "browserify": "browserify build/main.js -o dist/main.js",
    "uglifyjs": "exit && uglifyjs --screw-ie8 --compress -o dist/main.min.js dist/main.js",
    "predist": "npm run build",
    "dist": "npm run browserify && npm run uglifyjs",
    "lint": "eslint src",
    "fix": "eslint src --fix",
    "start": "node server.js"
  },
  "author": "widmogrod@gmail.com",
  "license": "ISC",
  "dependencies": {
    "bufferutil": "^3.0.2",
    "express": "^4.15.4",
    "js-crdt": "git+ssh://git@github.com/widmogrod/js-crdt.git#feature/set-map-to-ordered-ma",
    "quill": "^1.3.2",
    "utf-8-validate": "^3.0.3",
    "ws": "^3.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "browserify": "^13.0.1",
    "eslint": "^4.7.0",
    "queueing-subject": "^0.1.1",
    "rxjs": "^5.4.3",
    "rxjs-websockets": "^1.1.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2",
    "typescript-eslint-parser": "^3.0.0",
    "uglify-es": "^3.1.0",
    "uglify-js": "^2.6.4"
  },
  "engines": {
    "node": "8.2.1"
  }
}
